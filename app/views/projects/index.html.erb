<% provide(:title, 'Projects') %>
<h1>Projects</h1>

<center>
  <button type="button" class="btn btn-dark">
    <%= link_to "New Project", new_project_path %>
  </button>
</center>

<h2>All Projects</h2>

<ul class="users">
  <% @project.each do |project| %>
    <li>
      <%= link_to project.project_name, project %>
    </li>
  <% end %>
</ul>
